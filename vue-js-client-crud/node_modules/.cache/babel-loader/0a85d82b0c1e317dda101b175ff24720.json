 successfully!</h4>\r\n      <button class=\"btn btn-success\" @click=\"newPerson\">Add</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PersonDataService from '../services/PersonDataService';\r\n\r\nexport default {\r\n  name: 'add-person',\r\n  data() {\r\n    return {\r\n      /* here to change */\r\n      person: {\r\n        id: null,\r\n        title: '',\r\n        sex: '',\r\n        firstname: '',\r\n        lastname: '',\r\n        published: false,\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  methods: {\r\n    savePerson() {\r\n      var data = {\r\n        title: this.person.title,\r\n        sex: this.person.sex,\r\n        firstname: this.person.firstname,\r\n        lastname: this.person.lastname,\r\n      };\r\n\r\n      PersonDataService.create(data)\r\n        .then(response => {\r\n          this.person.id = response.data.id;\r\n          console.log(response.data);\r\n          this.submitted = true;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    newPerson() {\r\n      this.submitted = false;\r\n      this.person = {};\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.submit-form {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}