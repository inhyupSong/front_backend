td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"country\">Country</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"country\"\r\n                      required\r\n                      v-model=\"book.country\"\r\n                      name=\"country\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"email\">Email</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"email\"\r\n                      required\r\n                      v-model=\"book.email\"\r\n                      name=\"email\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"phone_nr\">Phone Nr.</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"phone_nr\"\r\n                      required\r\n                      v-model=\"book.phone_nr\"\r\n                      name=\"phone_nr\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"mobile_nr\">Mobile Nr.</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"mobile_nr\"\r\n 