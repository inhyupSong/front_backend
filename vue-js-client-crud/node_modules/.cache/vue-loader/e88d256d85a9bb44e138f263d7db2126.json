ass=\"btn btn-outline-secondary\"\r\n            type=\"button\"\r\n            @click=\"searchTitle\"\r\n          >\r\n            Search\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <h4>Books List</h4>\r\n      <ul class=\"list-group\">\r\n        <li\r\n          class=\"list-group-item\"\r\n          :class=\"{ active: index == currentIndex }\"\r\n          v-for=\"(book, index) in books\"\r\n          :key=\"index\"\r\n          @click=\"setActiveBook(book, index)\"\r\n        >\r\n          <div>\r\n            <tr>\r\n            <td>{{ book.title }} </td>\r\n            </tr>\r\n            <tr>\r\n            <td>{{ book.author }} </td>\r\n             </tr>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n\r\n      <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllBooks\">\r\n        Remove All\r\n      </button>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <div v-if=\"currentBook\">\r\n        <h4>Book</h4>\r\n        <div>\r\n          <label><strong>Title:</strong></label> {{ currentBook.title }}\r\n        </div>\r\n        <a class=\"badge badge-warning\" :href=\"'/books/' + currentBook.id\">\r\n          Edit\r\n        </a>\r\n      </div>\r\n      <div v-else>\r\n        <br />\r\n        <p>Please click on a Book...</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport BookDataService from '../services/BookDataService';\r\n\r\nexport default {\r\n  name: 'books-list',\r\n  data() {\r\n    return {\r\n      books: [],\r\n      currentBook: null,\r\n      currentIndex: -1,\r\n      title: '',\r\n    };\r\n  },\r\n  methods: {\r\n    retrieveBooks() {\r\n      BookDataService.getAll()\r\n        //BookDataService.fuck()\r\n        .then(response => {\r\n          this.books = response.data.books;\r\n\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    refreshList() {\r\n      this.retrieveBooks();\r\n      this.currentBook = null;\r\n      this.currentIndex = -1;\r\n    },\r\n    searchTitle() {\r\n      BookDataService.findByTitle(this.title)\r\n        .then(response => {\r\n          this.books = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.retrieveBooks();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.list {\r\n  text-align: left;\r\n  max-width: 750px;\r\n  margin: auto;\r\n}\r\n</style>\r\n\r\n"]}]}