\r\n          <button class=\"btn btn-outline-secondary\" type=\"button\" @click=\"searchTitle\">\r\n            Search\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <h4>People List</h4>\r\n      <ul class=\"list-group\">\r\n        <li\r\n          class=\"list-group-item\"\r\n          :class=\"{ active: index == currentIndex }\"\r\n          v-for=\"(person, index) in people\"\r\n          :key=\"index\"\r\n          @click=\"setActivePerson(person, index)\"\r\n        >\r\n          {{ person.title }}\r\n        </li>\r\n      </ul>\r\n\r\n      <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllPeople\">\r\n        Remove All\r\n      </button>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <div v-if=\"currentPerson\">\r\n        <h4>Person</h4>\r\n        <div>\r\n          <label><strong>Title:</strong></label> {{ currentPerson.title }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Description:</strong></label> {{ currentPerson.description }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Status:</strong></label> {{ currentPerson.published ? 'Published' : 'Pending' }}\r\n        </div>\r\n\r\n        <a class=\"badge badge-warning\" :href=\"'/people/' + currentPerson.id\">\r\n          Edit\r\n        </a>\r\n      </div>\r\n      <div v-else>\r\n        <br />\r\n        <p>Please click on a Person...</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PersonDataService from '../services/PersonDataService';\r\n\r\nexport default {\r\n  name: 'people-list',\r\n  data() {\r\n    return {\r\n      people: [],\r\n      currentPerson: null,\r\n      currentIndex: -1,\r\n      title: '',\r\n    };\r\n  },\r\n  methods: {\r\n    retrievePeople() {\r\n      PersonDataService.getAll()\r\n        .then(response => {\r\n          this.people = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    refreshList() {\r\n      this.retrievePeople();\r\n      this.currentPerson = null;\r\n      this.currentIndex = -1;\r\n    },\r\n\r\n    setActivePerson(person, index) {\r\n      this.currentPerson = person;\r\n      this.currentIndex = index;\r\n    },\r\n\r\n    removeAllPeople() {\r\n      PersonDataService.deleteAll()\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.refreshList();\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    searchTitle() {\r\n      PersonDataService.findByTitle(this.title)\r\n        .then(response => {\r\n          this.people = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.retrievePeople();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.list {\r\n  text-align: left;\r\n  max-width: 750px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"]}]}