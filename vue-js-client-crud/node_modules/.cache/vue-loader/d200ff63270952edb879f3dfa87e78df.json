book: {\r\n        id: null,\r\n        title: '',\r\n        sex: '',\r\n        firstname: '',\r\n        lastname: '',\r\n        street: '',\r\n        house_nr: '',\r\n        region: '',\r\n        post_nr: '',\r\n        state: '',\r\n        country: '',\r\n        email: '',\r\n        phone_nr: '',\r\n        mobile_nr: '',\r\n        extra_infos: '',\r\n\r\n        published: false,\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  methods: {\r\n    saveBook() {\r\n      var data = {\r\n        title: this.book.title,\r\n        sex: this.book.sex,\r\n        firstname: this.book.firstname,\r\n        lastname: this.book.lastname,\r\n        street: this.book.street,\r\n        house_nr: this.book.house_nr,\r\n        region: this.book.region,\r\n        post_nr: this.book.post_nr,\r\n        state: this.book.state,\r\n        country: this.book.country,\r\n        email: this.book.email,\r\n        phone_nr: this.book.phone_nr,\r\n        mobile_nr: this.book.mobile_nr,\r\n        extra_infos: this.book.extra_infos,\r\n      };\r\n\r\n      BookDataService.create(data)\r\n        .then(response => {\r\n          this.book.id = response.data.id;\r\n          console.log(response.data);\r\n          this.submitted = true;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    newBook() {\r\n      this.submitted = false;\r\n      this.book = {};\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.submit-form {\r\n  max-width: 80%;\r\n  margin: auto;\r\n}\r\n\r\n.field-table{\r\n  margin-top: 3rem;\r\n  margin-left: 10rem;\r\n}\r\n\r\n#submit-button{\r\n  margin-right: 160px;\r\n}\r\n</style>\r\n"]}]}