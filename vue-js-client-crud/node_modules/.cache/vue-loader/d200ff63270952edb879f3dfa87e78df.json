  v-model=\"book.title\"\r\n                      name=\"title\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"sex\">Sex</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"sex\"\r\n                      required\r\n                      v-model=\"book.sex\"\r\n                      name=\"sex\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"firstname\">First Name</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"firstname\"\r\n                      required\r\n                      v-model=\"book.firstname\"\r\n                      name=\"firstname\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"lastname\">Last Name</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"lastname\"\r\n                      required\r\n                      v-model=\"book.lastname\"\r\n                      name=\"lastname\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"street\">Street</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"street\"\r\n                      required\r\n                      v-model=\"book.street\"\r\n                      name=\"street\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"house_nr\">House Nr.</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"house_nr\"\r\n                      required\r\n                      v-model=\"book.house_nr\"\r\n                      name=\"house_nr\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"region\">Region</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"region\"\r\n                      required\r\n                      v-model=\"book.region\"\r\n                      name=\"region\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n              </table>\r\n            </div>\r\n\r\n            <div class=\"col\">\r\n              <table>\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"post_nr\">Post Nr.</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"post_nr\"\r\n                      required\r\n                      v-model=\"book.post_nr\"\r\n                      name=\"post_nr\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"state\">State</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"state\"\r\n                      required\r\n                      v-model=\"book.state\"\r\n                      name=\"state\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"country\">Country</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"country\"\r\n                      required\r\n                      v-model=\"book.country\"\r\n                      name=\"country\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"email\">Email</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"email\"\r\n                      required\r\n                      v-model=\"book.email\"\r\n                      name=\"email\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"phone_nr\">Phone Nr.</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"phone_nr\"\r\n              