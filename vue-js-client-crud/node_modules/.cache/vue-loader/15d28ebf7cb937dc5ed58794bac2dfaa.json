   v-model=\"person.title\"\r\n                      name=\"title\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"sex\">Sex</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"sex\"\r\n                      required\r\n                      v-model=\"person.sex\"\r\n                      name=\"sex\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"firstname\">First Name</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"firstname\"\r\n                      required\r\n                      v-model=\"person.firstname\"\r\n                      name=\"firstname\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"lastname\">Last Name</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"lastname\"\r\n                      required\r\n                      v-model=\"person.lastname\"\r\n                      name=\"lastname\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"street\">Street</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"street\"\r\n                      required\r\n                      v-model=\"person.street\"\r\n                      name=\"street\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"house_nr\">House Nr.</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"house_nr\"\r\n                      required\r\n                      v-model=\"person.house_nr\"\r\n                      name=\"house_nr\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"region\">Region</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"region\"\r\n                      required\r\n                      v-model=\"person.region\"\r\n                      name=\"region\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n              </table>\r\n            </div>\r\n\r\n            <div class=\"col\">\r\n              <table>\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"post_nr\">Post Nr.</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"post_nr\"\r\n                      required\r\n                      v-model=\"person.post_nr\"\r\n                      name=\"post_nr\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"state\">State</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"state\"\r\n                      required\r\n                      v-model=\"person.state\"\r\n                      name=\"state\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"country\">Country</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"country\"\r\n                      required\r\n                      v-model=\"person.country\"\r\n                      name=\"country\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"email\">Email</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"email\"\r\n                      required\r\n                      v-model=\"person.email\"\r\n                      name=\"email\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"phone_nr\">Phone Nr.</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"phone_nr\"\r\n                      required\r\n                      v-model=\"person.phone_nr\"\r\n                      name=\"phone_nr\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"mobile_nr\">Mobile Nr.</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"mobile_nr\"\r\n                      required\r\n                      v-model=\"person.mobile_nr\"\r\n                      name=\"mobile_nr\"\r\n                    />\r\n                  </td><br /><br />\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td>\r\n                    <label for=\"extra_infos\">Extra Infos</label>\r\n                  </td>\r\n                  <td>\r\n                    <input\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"extra_infos\"\r\n                      required\r\n                      v-model=\"person.extra_infos\"\r\n                      name=\"extra_infos\"\r\n                    />\r\n                  </td><br /><br />\r\n                  \r\n                </tr>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </form>\r\n        <br /><br />\r\n        <div id=\"submit-button\">\r\n        <button @click=\"savePerson\" class=\"btn btn-success\">Submit</button>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-else>\r\n        <h4>You submitted successfully!</h4>\r\n        <button class=\"btn btn-success\" @click=\"newPerson\">Add</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport PersonDataService from '../services/PersonDataService';\r\n\r\nexport default {\r\n  name: 'add-person',\r\n  data() {\r\n    return {\r\n      /* here to change */\r\n      person: {\r\n        id: null,\r\n        title: '',\r\n        sex: '',\r\n        firstname: '',\r\n        lastname: '',\r\n        street: '',\r\n        house_nr: '',\r\n        region: '',\r\n        post_nr: '',\r\n        state: '',\r\n        country: '',\r\n        email: '',\r\n        phone_nr: '',\r\n        mobile_nr: '',\r\n        extra_infos: '',\r\n\r\n        published: false,\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  methods: {\r\n    savePerson() {\r\n      var data = {\r\n        title: this.person.title,\r\n        sex: this.person.sex,\r\n        firstname: this.person.firstname,\r\n        lastname: this.person.lastname,\r\n        street: this.person.street,\r\n        house_nr: this.person.house_nr,\r\n        region: this.person.region,\r\n        post_nr: this.person.post_nr,\r\n        state: this.person.state,\r\n        country: this.person.country,\r\n        email: this.person.email,\r\n        phone_nr: this.person.phone_nr,\r\n        mobile_nr: this.person.mobile_nr,\r\n        extra_infos: this.person.extra_infos,\r\n      };\r\n\r\n      PersonDataService.create(data)\r\n        .then(response => {\r\n          this.person.id = response.data.id;\r\n          console.log(response.data);\r\n          this.submitted = true;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    newPerson() {\r\n      this.submitted = false;\r\n      this.person = {};\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.submit-form {\r\n  max-width: 80%;\r\n  margin: auto;\r\n}\r\n\r\n.field-table{\r\n  margin-top: 3rem;\r\n  margin-left: 10rem;\r\n}\r\n\r\nsubmit-button{\r\n  margin-right: -20px\r\n}\r\n</style>\r\n"]}]}